{
    "manifest_version": 2,
    "name": "Ken's Custom New-Tab",
    "description": "A minimalistic custom new-tab page built by Ken and for Ken.",
    "version": "1.0.0",
    "permissions" : [
        "management",
        "storage",
        "history"
    ],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "web_accessible_resources": [
        "newtab.css",
        "css/normalize.css",
        "fonts/font-awesome-4.2.0/css/font-awesome.min.css",
        "css/demo.css",
        "css/set1.css"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com http://127.0.0.1:5000 https://dining.brown.edu/cafe/sharpe-refectory/; object-src 'self'"
 }